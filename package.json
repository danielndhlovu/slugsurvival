{
  "name": "slugsurvival",
  "version": "3.18.0",
  "scripts": {
    "dev": "node node_modules/watchify/bin/cmd.js -vd -p browserify-hmr -e src/main.js -o public/app.js -v",
    "test": "node node_modules/.bin/mocha --timeout 30000 --reporter spec",
    "prod": "gulp && NODE_ENV=production node ./node_modules/browserify/bin/cmd.js -g envify -e src/main.js | node ./node_modules/uglify-js/bin/uglifyjs -c warnings=false -m > public/prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.fm:zllovesuki/slugsurvival.git"
  },
