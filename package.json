  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "~3.1.0",
    "express": "^4.16.2",
    "fill-pdf": "^1.1.0",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "alertify.js": "^1.0.12",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "browserify": "^15.2.0",
    "browserify-hmr": "^0.3.6",
    "compound-subject": "0.0.2",
    "envify": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "localforage": "^1.5.6",
    "lodash.debounce": "^4.0.8",
    "lunr": "^2.1.5",
    "mobile-detect": "^1.4.1",
    "stringify": "^5.2.0",
    "uglify-js": "^3.3.9",
    "v-tooltip": "^2.0.0-rc.30",
    "vue": "^2.5.13",
    "vue-hot-reload-api": "^2.2.4",
    "vue-resource": "^1.3.5",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1",
