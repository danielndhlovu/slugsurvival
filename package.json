{
  "name": "slugsurvival",
  "version": "3.26.0",
  "scripts": {
    "dev": "node node_modules/watchify/bin/cmd.js -vd -p browserify-hmr -e src/main.js -o public/app.js -v",
    "test": "node node_modules/.bin/mocha --timeout 30000 --reporter spec",
    "prod": "gulp && NODE_ENV=production node ./node_modules/browserify/bin/cmd.js -t vueify -t stringify -g envify -e src/main.js -d | node ./node_modules/terser/bin/uglifyjs -o public/prod.js -c warnings=false -m --source-map"
  },
  "repository": {
    "type": "git",
    "url": "git@git.fm:zllovesuki/slugsurvival.git"
  },
  "author": "Rachel Chen <me@rachelchen.me>",
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.2",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.6",
    "compound-subject": "0.0.2",
    "envify": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "localforage": "^1.7.2",
    "lodash.debounce": "^4.0.8",
    "lunr": "^2.3.4",
    "mobile-detect": "^1.4.3",
    "stringify": "^5.2.0",
    "superagent": "^4.0.0-beta.5",
    "terser": "^3.10.1",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1",
    "vueify-insert-css": "^1.0.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "watchify": "^3.11.0"
  }
